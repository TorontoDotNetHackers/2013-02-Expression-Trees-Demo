<?xml version="1.0" encoding="utf-8" ?>

<program>

	<printLine value="This is a demo of the TDNH-XML# language being compiled and run." />
	<printLine value="Sample source code files are in the folder project source/*.xml" />
	<printLine value="" />
	<printLine value="** Display Hello 10x **" />
	<printLine value="" />

	<for varName="x" initValue="0" lessThan="10" incrementBy="1">
		<printLine value="Hello" />
	</for>

	<printLine value="** Display World 5x **" />

	<for varName="y" initValue="0" lessThan="5" incrementBy="1">
		<printLine value="World" />
	</for>

	<printLine value="" />
	<printLine value="The End" />

</program>
